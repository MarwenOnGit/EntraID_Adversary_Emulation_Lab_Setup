# Ansible Inventory - Red Team Infrastructure
# Generated from Terraform outputs or manually populated

[redteam]
# Add your instances here after terraform apply
# Example:
# redteam-01 ansible_host=<public_ip> ansible_user=ec2-user
# redteam-02 ansible_host=<public_ip> ansible_user=ec2-user

[redteam:vars]
ansible_user=ec2-user
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_connection=ssh
ansible_port=22
